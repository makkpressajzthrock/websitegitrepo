function remove_temp_speed_data($conn,$website_id,$parent_website=0) {
	// now delete the temp_pagespeed_report data 
	if ( empty($parent_website) ) {
		$query3 = $conn->query("DELETE FROM temp_pagespeed_report WHERE website_id = '$website_id'; ");
	}
	else {
		$query3 = $conn->query("DELETE FROM temp_pagespeed_report WHERE website_id = '$website_id' AND parent_website = '$parent_website'; ");
	}
}

function update_pagespeed_record($conn,$website_id,$highest_mobile_reanalyze,$highest_desktop_reanalyze,$speedtype,$parent_website=0) {

