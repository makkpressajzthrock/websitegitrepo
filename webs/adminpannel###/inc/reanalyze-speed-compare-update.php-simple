		if ( empty($parent_website) ) {
			$query1 = $conn->query(" SELECT * FROM temp_pagespeed_report WHERE `website_id` = '$website_id' AND plateform LIKE 'desktop' AND reanalyze_count = '$highest_desktop_reanalyze' ; ");
